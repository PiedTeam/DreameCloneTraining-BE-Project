{
  "name": "dreame-clone-training-be",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx nodemon",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "lint": "yarn run eslint:check && yarn run prettier:check && yarn run check-types",
    "start:dev": "cross-env NODE_ENV=development node dist/index.js",
    "start:prod": "cross-env NODE_ENV=production node dist/src/index.js",
    "prepare": "husky",
    "prettier:check": "prettier ./src/**/*.{tsx,ts,js,jsx} --check",
    "prettier:fix": "prettier ./src/**/*.{tsx,ts,js,jsx} --write",
    "eslint:check": "eslint ./src --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "eslint:fix": "eslint ./src --ext .ts,.tsx,.js,.jsx --max-warnings 0 --fix",
    "eslint:check-allow-warning": "eslint ./src --ext .ts,.tsx,.js,.jsx",
    "check-conflict-prettier-eslint": "npx eslint-config-prettier src/App.tsx --print-config",
    "check-types": "tsc --noemit"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "formidable": "^3.5.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.8",
    "mongodb": "^5.7.0",
    "nodemailer": "^6.9.10",
    "openapi-types": "^12.1.3",
    "sharp": "^0.33.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "yaml": "^2.4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@faker-js/faker": "^8.2.0",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@tsconfig/node16": "^16.1.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.197",
    "@types/minimist": "^1.2.3",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@typescript-eslint/types": "^7.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
